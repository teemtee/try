
# Mutliple discover configs using ref
#
# Execution can look like this
# tmt run -e REF=main discover -v

discover:
  - name: grep
    how: fmf
    url: https://src.fedoraproject.org/tests/grep
    ref: $REF
  - name: httpd
    how: fmf
    url: https://src.fedoraproject.org/tests/httpd
    ref: $REF
  - name: lftp
    how: fmf
    url: https://src.fedoraproject.org/tests/lftp
    ref: $REF
execute:
    how: tmt
